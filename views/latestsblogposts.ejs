<%= include header.ejs %>
<div class="container">
  <%
  data.forEach(function(data, i) {
  %>
    <div class="row">
      <div class="span8">
        <h1><a href="http://<%=subdomain%>.<%=domain%>/<%=data.url%>"><%=data.title%></a></h1>
        <p>
          <%-data.content.replace("\n", "<br />")%>
        </p>
      </div>
    </div>
  <%
  });
  %>
</div>

<%= include footer.ejs %>